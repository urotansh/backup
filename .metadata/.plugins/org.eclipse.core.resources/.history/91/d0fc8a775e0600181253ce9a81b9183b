import java.util.*;

public class Sample {{
		
		public static void main(String[] args){
			ArrayList al=new ArrayList();
			al.add(new Student("安藤",1,1));
			al.add(new Student("伊藤",1,2));
			al.add(new Student("太田",2,3));
			al.add(new Student("加藤",2,4));
			al.add(new Student("木田",3,5));
			
			al.remove(3);
			for(int i=0;i<al.size();i++){
				Student s=(Student)al.get(i);
				System.out.println("番号:"+s.getNumber()+"名前:"+s.getName()+"学年"+s.getGrade());
			}
		}
	}

}
